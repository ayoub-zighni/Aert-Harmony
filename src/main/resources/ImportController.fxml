<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="300" prefWidth="400" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ImportController"> <!-- Ajustez la taille de la fenêtre ici -->
    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>
    <children>
        <Button fx:id="importButton" onAction="#importer" prefHeight="42.0" prefWidth="143.0" text="Importer" /> <!-- Ajustez la taille du bouton ici -->
        <Button fx:id="genererQRCodeButton" layoutX="122.0" layoutY="562.0" mnemonicParsing="false" onAction="#genererQRCode" prefHeight="40.0" prefWidth="142.0" text="Générer QR Code" />
        <ImageView fitHeight="218.0" fitWidth="141.0">
            <image>
                <Image url="@Event.png" />
            </image>
        </ImageView>
    </children>
</VBox>
